@model NossoBrasil.Dtos.EnderecoDTO

@{
    ViewData["Title"] = "Pesquisar";
}

<div class="card">

    <div class="card-header bg-primary">
        <h1 class="card-title text-light text-center">Endereço</h1>
    </div>

    <div class="card-body"> 
   
        <dl class="row">
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Cep)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Cep)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Estado)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Estado)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Cidade)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Cidade)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Bairro)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Bairro)
            </dd>
            <dt class = "col-sm-2">
                @Html.DisplayNameFor(model => model.Logradouro)
            </dt>
            <dd class = "col-sm-10">
                @Html.DisplayFor(model => model.Logradouro)
            </dd>
        
        </dl>
    </div>
    <div class="card-footer">
        <a asp-action="Index" class="btn btn-info btn-lg">Voltar</a>
    </div>
</div>

